{
  "name": "weapper",
  "version": "1.1.0",
  "description": "A custom compiler for miniprogram development.",
  "main": "build/weapper.js",
  "scripts": {
    "meta": "babel-node scripts/build-meta.js",
    "build": "npm run meta && babel -s -d build src",
    "dev": "npm run build -- -w",
    "sync:down": "rsync -r -v --update --exclude=node_modules --exclude=build root@ichvv.com:~/Develop/weapper/* ./",
    "sync:up": "rsync -r -v --update --exclude=node_modules --exclude=build ./* root@ichvv.com:~/Develop/weapper/"
  },
  "keywords": [
    "wechat",
    "miniprogram",
    "babel",
    "scss",
    "compiler",
    "graph"
  ],
  "author": "程巍巍",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime-corejs3": "^7.4.4",
    "@babel/types": "^7.4.4",
    "babel-preset-minify": "^0.5.0",
    "color": "^3.1.1",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "fs-extra": "^8.0.0",
    "postcss": "^7.0.16",
    "sass": "^1.20.1",
    "yaml": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-flow": "^7.0.0"
  }
}
